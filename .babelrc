{
  "presets": [
    "react",
    "es2015",
    "stage-2"
  ],
  "env": {
    "test": {
      "presets": [
        "react",
        "es2015",
        "stage-2"
      ],
      "plugins": [
        "istanbul",
        "transform-object-rest-spread",
        "react-hot-loader/babel"
      ]
    },
    "production": {
      "plugins": [
        "transform-object-rest-spread"
      ],
      "presets": [
        [
          "es2015",
          {
            "modules": false
          }
        ],
        "stage-2"
      ]
    }
  }
}
